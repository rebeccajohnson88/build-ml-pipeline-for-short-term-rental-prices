name: basic_cleaning
conda_env: conda.yml

entry_points:
  main:
    parameters:
      input_artifact: 
        description: fully qualified name of artifact
        type: str
      output_artifact: 
        description: name of artifact we're creating from script
        type: str
      output_type:
        description: Type of artifact to create
        type: str
        default: clean_sample
      output_description:
        description: Description for output artifact
        type: str
      min_price:
        description: minimum price for rental 
        type: float
      max_price:
        description: maximum price for rental
        type: float

    command: >-
        python run.py --input_artifact {input_artifact} \
                      --output_artifact {output_artifact} \
                      --output_type {output_type} \
                      --output_description {output_description} \
                      --min_price {min_price} \ 
                      --max_price {max_price}

